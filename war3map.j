local integer pos=offset
local string s
local string str
if(offset<1)then
set pos=1
endif
if(find=="")then
return-1
endif
loop
set s=SubString(subject,pos-1,pos+len-1)
if(s==find)then
return pos
endif
if(SubString(subject,pos-1,pos)=="")then
return-1
endif
set pos=pos+1
endloop
return-1
endfunction
